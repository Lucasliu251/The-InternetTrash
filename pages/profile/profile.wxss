page {
  background-color: #080a0d; /* 设置背景颜色为黑色 */
}


.flex{
    display: flex;
}
.flex-1{
    flex: 1 1 0%;
}
.flex-col{
    flex-direction: column;
}
.flex-grow{
  flex-grow: 1;
}

.items-center{
  align-items: center;
}

.text-xl{
    font-size: 1.25rem;
    line-height:1.75rem;
}
.text-lg{
    font-size: 1.125rem;
    line-height:1.75rem;
}
.text-sm{
    font-size: 0.875rem;
    line-height:1.25rem;
}
.text-xs{
    font-size: 0.75rem;
    line-height:1rem;
}
.text-text-primary{
  --tw-text-opacity: 1;
  color: rgb(248 249 251/var(--tw-text-opacity))
}
.text-text-secondary{
    --tw-text-opacity: 1;
    color: rgb(156 173 196/var(--tw-text-opacity))
}

.font-medium{
    font-weight: 500;
}


.leading-6{
    line-height: 1.5rem;
}
.relative{
  position:relative;
}

.rounded-full{
    border-radius: 9999px;
}
.rounded-md{
    border-radius: 0.375rem;
}
.w-full{
  width:100%;
}

.rounded-lg{
  border-radius: 0.5rem;
}


.gap-1{
    gap: 0.25rem;
}
.gap-2{
  gap: 0.5rem;
}
.gap-4{
    gap: 1rem;
}
.gap-9{
    gap: 2.25rem;
}

.w-16{
    width: 4rem;
}
.h-16{
    height: 4rem; 
}
.pb-16{
  padding-bottom: 4rem;
}
.px-3{
  padding-left: 0.75rem;
  padding-right: 0.75rem;
}
.px-6{
    padding-left: 1.5rem;
    padding-right: 1.5rem;
}
.py-205{
    padding-top: 0.625rem;
    padding-bottom: 0.625rem;
}
.py-4{
  padding-top: 1rem;
  padding-bottom: 1rem;
}
.py-6{
    padding-top: 1.5rem;
    padding-bottom: 1.5rem;
}
.mt-1{
    margin-top: 0.25rem;
}

.border-b{
    border-bottom-width: 1px;
}

.max-w-2xl{
    max-width: 42rem;
}


.border-gray-800{
  --tw-border-opacity: 1;
  border-color: rgb(30 39 51/var(--tw-border-opacity))
}
.divide-gray-800{
  --tw-divide-opacity: 1;
}
.bg-gray-800{
  --tw-bg-opacity: 1;
  background-color: rgb(30 39 51/var(--tw-bg-opacity))
}
.bg-gray-900{
    --tw-bg-opacity: 1;
    background-color: rgb(15 20 26/var(--tw-bg-opacity))
}
